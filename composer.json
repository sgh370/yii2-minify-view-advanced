{
  "name": "sgh370/yii2-minify-view-advanced",
  "description": "Advanced Yii2 Minify View , Css And Js",
  "keywords": [
    "yii2",
    "minify",
    "view",
    "js",
    "css",
	"advanced",
	"comporessor"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Saeed Ghanbari",
      "email": "sgh370@yahoo.com",
      "homepage": "http://saeedghanbari.com/"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0",
    "mrclay/minify": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "sgh370\\yii\\minify\\": ""
    }
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      },
      "vcs-driver-options": {
        "github-no-api": true
      },
      "pattern-skip-version": "(-build|-patch)"
    }
  }
}